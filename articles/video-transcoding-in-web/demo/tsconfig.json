{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "outDir": "./dist",
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
}